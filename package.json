{
  "name": "next-graphql",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "graphql-codegen --require dotenv/config --config codegen.yml",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "concurrently \"next dev\" \"graphql-codegen --require dotenv/config --config codegen.yml --watch graphql/*.graphql\"",
    "preserve": "next build",
    "serve": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mdx-js/react": "^1.6.22",
    "@react-hookz/web": "^12.0.0",
    "@react-spring/web": "^9.3.0",
    "graphql": "^16.2.0",
    "graphql-request": "^3.7.0",
    "next": "^12.0.7",
    "next-i18next": "^10.1.0",
    "next-seo": "^4.28.1",
    "nextjs-progressbar": "^0.0.13",
    "polished": "^4.1.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "theme-ui": "^0.13.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.3.1",
    "@graphql-codegen/typescript": "^2.4.2",
    "@graphql-codegen/typescript-graphql-request": "^4.3.3",
    "@graphql-codegen/typescript-operations": "^2.2.2",
    "@next/bundle-analyzer": "^12.0.2",
    "@types/node": "16.11.6",
    "@types/react": "17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "concurrently": "^6.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "7",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "next-sitemap": "^1.6.203",
    "typescript": "4.4.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "version": "1.0.1"
}
